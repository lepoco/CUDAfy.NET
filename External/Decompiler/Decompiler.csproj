<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <TargetFrameworks>net48;net5.0-windows;net6.0-windows</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <PlatformTarget>x64</PlatformTarget>
    <Platforms>x64</Platforms>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Deterministic>true</Deterministic>
    <PackageVersion>0.0.0</PackageVersion> <!-- TODO: Find base Decompiler Version -->
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWpf>true</UseWpf>
    <NoStdLib>False</NoStdLib>
  </PropertyGroup>

  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageProjectUrl>https://github.com/icsharpcode/ILSpy</PackageProjectUrl>
    <RepositoryUrl>https://github.com/icsharpcode/ILSpy</RepositoryUrl>
    <PackageLicense>MIT License</PackageLicense>
  </PropertyGroup>

  <PropertyGroup>
    <Copyright>Copyright 2011-2021 AlphaSierraPapa for the SharpDevelop Team</Copyright>
  </PropertyGroup>

</Project>

